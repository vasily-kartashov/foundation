<VirtualHost *:80>
    ServerAdmin webmaster@localhost
    ServerName foundation.dev
    DocumentRoot /vagrant/desktop/public
    <Directory />
        Options FollowSymLinks
        AllowOverride None
    </Directory>
    <Directory /vagrant/desktop/public>
        Options Indexes FollowSymLinks MultiViews
        AllowOverride All
        Order allow,deny
        allow from all
    </Directory>
    ErrorLog /vagrant/logs/error.log
    LogLevel warn
    CustomLog /vagrant/logs/access.log combined
</VirtualHost>